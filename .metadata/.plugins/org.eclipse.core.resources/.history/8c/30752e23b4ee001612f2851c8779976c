#include"check_password.h"

uint8 check_pass(uint8*recv,uint8*eeprom)
{
	uint8 flag=0;
	while(*recv !='\0')
	{
		if(*recv != *eeprom)
		{
			flag=1;
		}
		else
		{

		}
		recv++;
		eeprom++;

	}
	if(flag==1)
	{
		return 0;//0 that mean the password is written by user is false
	}
	else
	{
		return 1; // 1 that mean the password is right
	}
}
